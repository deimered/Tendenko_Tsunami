            </ui:VisualElement>
            <ui:VisualElement name="back_option" style="justify-content: center; align-items: center; height: 19%;">
                <ui:Button text="Back" display-tooltip-when-elided="true" name="back" class="button_pause" style="height: 100%; width: 34%; margin-top: 0; margin-bottom: 0;" />
